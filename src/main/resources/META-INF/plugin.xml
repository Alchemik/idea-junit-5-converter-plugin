<idea-plugin>
    <id>de.wlsc.junit.converter.plugin</id>
    <name>JUnit4-to-5-Converter</name>
    <version>1.0</version>
    <vendor url="https://wlsc.de">wlsc.de</vendor>
    <description>JUnit 4 to 5 converter</description>
